{
  "name": "hashbrown-cms",
  "repository": "https://github.com/HashBrownCMS/hashbrown-cms.git",
  "version": "1.2.0",
  "description": "A free and open-source headless CMS",
  "main": "hashbrown.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack --progress",
    "build:dev": "webpack --watch --watch-poll --progress --no-babel",
    "start": "npm run build && node hashbrown.js",
    "start:nodemon": "nodemon hashbrown.js",
    "start:docker": "cd ./docker && sudo docker-compose up --build -d",
    "stop:docker": "cd ./docker && sudo docker-compose down"
  },
  "author": "Putaitu",
  "license": "MIT",
  "dependencies": {
    "app-module-path": "^2.2.0",
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.3",
    "express": "^4.16.4",
    "glob": "^7.1.3",
    "js-beautify": "^1.8.9",
    "mongodb": "^3.1.10",
    "multer": "^1.4.1",
    "pug": "^2.0.3",
    "semver": "^5.6.0",
    "@babel/core": "^7.2.2",
    "@babel/polyfill": "^7.4.3",
    "@babel/preset-env": "^7.2.3",
    "babel-loader": "^8.0.5",
    "json-loader": "^0.5.4",
    "sass": "^1.16.0",
    "webpack": "^4.28.1",
    "webpack-cli": "^3.3.0"
  }
}
